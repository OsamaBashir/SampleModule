<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_WebapiCors:etc/cors.xsd">
    <cors>
        <route url="/V1/*">
            <allow_origin>*</allow_origin>
            <allow_methods>GET,POST,PUT,DELETE,OPTIONS</allow_methods>
            <allow_headers>*</allow_headers>
            <expose_headers>*</expose_headers>
            <max_age>3600</max_age>
        </route>
    </cors>
</config>





